pipeline{
    agent any
    tools{
        maven 'm1'
    }
    stages{
        stage('test'){
        
            steps{
                sh 'mvn test'
            }
        }
        stage('install'){
            steps{
                sh 'mvn clean install -D skipTests=true'
            }
        }
stage('sonar'){
                steps{
                        withSonarQubeEnv('sonar'){
                        sh 'mvn clean install sonar:sonar'
                        }
                        }
                }
        stage("jfrog"){
            steps{
                rtUpload(
                    serverId: 'jfrog',
                    spec: '''{
                        "files": [
                          {
                                "pattern": "**/**/*.*ar",
                                "target": "proj/"
                            }
                        ]
                    }'''
                )
            }
        }

    stage('Build') {
            steps {
                // Checkout your source code from a version control system like Git
               // checkout scm

                // Build your application (replace with your build commands)
                sh 'docker build -t frontend_dock:v1 .'
                sh 'docker build -t backend_dock:v1 . '
            }
}

}

