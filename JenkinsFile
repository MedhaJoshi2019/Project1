pipeline{
    agent any
    tools{
        maven 'm1'
    }
    stages{
        stage('test'){
        
            steps{
                sh 'mvn test'
            }
        }
        stage('install'){
            steps{
                sh 'mvn clean install -DskipTests=true'
            }
        }
stage('sonar'){
                steps{
                        withSonarQubeEnv('sonar'){
                        sh 'mvn clean install sonar:sonar'
                        }
                        }
                }
        stage("jfrog"){
            steps{
                rtUpload(
                    serverId: 'my-jfrog',
                    spec: '''{
                        "files": [
                          {
                                "pattern": "**/**/*.*ar",
                                "target": "proj-my-frog/"
                            }
                        ]
                    }'''
                )
            }
        }
 }

}

